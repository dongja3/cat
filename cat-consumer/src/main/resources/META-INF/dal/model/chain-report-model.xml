<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.consumer.chain.model" enable-merger="true" enable-sax-parser="true"
	enable-base-visitor="true"  enable-native-parser="true"  enable-native-builder="false">
	<entity name="chain-report" root="true">
		<attribute name="domain" value-type="String" key="true" />
		<entity-ref name="chain" type="map" names="chains" method-find-or-create="true" />
	</entity>

	<entity name="chain" class-name="TransactionChain">
		<attribute name ="level" value-type="int"/>
		<attribute name="domain" value-type="String" />
		<attribute name="transactionName" value-type="String" key="true" />
		<attribute name="callCount" value-type="long" primitive="true" method-inc="true" />
		<attribute name="avg" value-type="double" primitive="true" format="0.0" />
		<attribute name="sum" value-type="double" primitive="true" format="0.0" />
		<attribute name="dependency" value-type="double" primitive="true" format="0.0000" />
		<attribute name="timeRatio" value-type="double" primitive="true" format="0.0000" />
		<entity-ref name="chain" type="list" names="children" method-find-or-create="true" />
	</entity>

</model>

